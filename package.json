{
  "name": "webtorrent-vue",
  "description": "Webtorrent client with Vue.js",
  "version": "0.0.1",
  "author": "korowiov87",
  "private": true,
  "scripts": {
    "build_WORKING": "cross-env NODE_ENV=production browserify -g browserify-css -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c -m > dist/build.js",
    "build": "cross-env NODE_ENV=production browserify -g browserify-css -e src/main.js  > dist/build.js",
    "build2": "cross-env NODE_ENV=production browserify -g browserify-css -e src/main.js | uglifyjs -c -m > dist/build.js",
    "serve": "http-server -o -c 1 -a localhost"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "browserify-css"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "browserify": "^17.0.0",
    "browserify-css": "^0.15.0",
    "envify": "^4.1.0",
    "http-server": "^0.12.3",
    "https-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "render-media": "^4.1.0",
    "stream-http": "^3.2.0",
    "streamsaver": "^1.0.1",
    "uglify": "^0.1.5",
    "uglify-js": "^3.13.4",
    "vue": "^2.6.12",
    "vue-global-var": "^1.0.0",
    "vue-material": "^1.0.0-beta-15",
    "vue-router": "^3.5.1",
    "webtorrent": "^0.112.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "babelify": "^8.0.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "file-loader": "^6.2.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "vue-loader": "^14.2.3",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "vueify": "^9.4.1"
  }
}
